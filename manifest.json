{
  "description": "Application qui permet de s'avoir a qui appartient le media que l'on consulte",
  "manifest_version": 3,
  "name": "media owner",
  "version": "1.0",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png"
  }
}
